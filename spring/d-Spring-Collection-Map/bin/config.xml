<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="beanId" class="spring.NumberBean">
		<property name="data">
			<map>
				<entry key="1">
					<value>100</value>
				</entry>
				<entry key="2">
					<value>200</value>
				</entry>	
				<entry key="3">
					<value>300</value>
				</entry>			
			</map>
		</property>
	</bean>
</beans> 
  