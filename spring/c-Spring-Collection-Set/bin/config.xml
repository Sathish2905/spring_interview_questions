<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="beanId" class="spring.CountryBean">
		<property name="countries">
			<set>
				<value>INDIA</value>
				<value>USA</value>
				<value>UK</value>
				<value>CANADA</value>		
			</set>
		</property>
	</bean>
</beans> 
  